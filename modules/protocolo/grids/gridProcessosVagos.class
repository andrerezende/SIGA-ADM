<?
MIOLO::Import('modules::protocolo::controls::linkprocessovago');

class gridProcessosVagos extends MGrid
{
    /**
     * Base URL
     * @var string 
     */
    protected $listURL;
    /**
     * Constructor
     */
    function __construct($processos)
    {
        global $MIOLO, $self, $module;
        $linkprocessovago = new LinkProcessoVago('linkprocessovago','%1%');
        $href_grid = $MIOLO->GetActionURL($module,$self,'', Array('event'=>'btnConsulta:click'));
        $columns = array(
        	
            new MGridColumn('dataAbertura','center',false,'',false),
            new MGridControl($linkprocessovago,'Numero do Processo','center','','30%'),
            new MGridColumn('Tipo de processo','center',false,'35%',true),
            new MGridColumn('Data de Abertura','center',false,'35%',true),
            
        );
        $this->SetFilter(false);
        $this->SetIndex(1);
        parent::__construct($processos,$columns,$href_grid,$pageLength = 20);
        $this->SetTitle("Resultado da Busca por Processos Vagos");
        $this->SetWidth('100%');
        $this->SetLinkType('hyperlink');
    }
    function GenerateHeader()
    {
        return NULL;
    }
}
?>
