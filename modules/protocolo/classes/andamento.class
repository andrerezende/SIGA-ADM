class BusinessProtocoloAndamento extends Business
{
    public $numProcesso;
    public $numAndamento;
    public $dataEntrada;
    public $dataSaida;
    public $dataDespacho;
    public $status;
    public $setor;
    public $setorDestino;
    public $decisao;
    public $autor;
    public $processo;
    public $setorData;
    public $setorDestinoData;
    public $decisaoData;
	public $userEntrada;
	public $userSaida;
    
    function __construct($data=NULL)
    {
//      construct 
        $this->Business('sigaept',$data);
    }
    function setData($data)
    {
        $this->dataEntrada = ($data->dataEntrada) ? $data->dataEntrada : $this->dataEntrada;
        $this->dataSaida = ($data->dataSaida) ? $data->dataSaida : $this->dataSaida;
        $this->dataDespacho = ($data->dataDespacho) ? $data->dataDespacho : $this->dataDespacho;
        $this->status = ($data->status) ? $data->status : $this->status;
        $this->setor = ($data->setor) ? $data->setor : $this->setor;
        $this->setorDestino = ($data->setorDestino)? $data->setorDestino : $this->setorDestino;
        $this->decisao = ($data->decisao) ? $data->decisao : $this->decisao;
        $this->autor = ($data->autor) ? $data->autor : $this->autor;
		$this->userEntrada = ($data->userEntrada) ? $data->userEntrada : $this->userEntrada;
		$this->userSaida = ($data->userSaida) ? $data->userSaida : $this->userSaida;
    }
}
