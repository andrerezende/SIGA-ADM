<?
class frmProcessoTramite extends MForm
{
    protected $processo;
    function __construct($objProcesso)
    {
//      construct
        global $MIOLO;
		$this->processo = $objProcesso;
        parent::__construct('Trâmite do processo');
        $this->SetClose($MIOLO->GetActionURL('protocolo','main'));
        $this->eventHandler();
    }
	function CreateFields()
    {
//      get grid
        $grid = $this->manager->GetUI()->GetGrid('protocolo','gridProcessosTramite',$this->processo);
        $this->AddField($grid);
//      buttons
        $this->defaultButton = false;
	}
	function btnReport_click()
	{  
		$ui = $this->manager->GetUI();
		if ( $this->processo->andamento )
		{
	        global $MIOLO;
	        $report = new MJasperReport('sigaept');
	        $parameters['SUBREPORT_DIR'] = $MIOLO->GetConf('home.modules');
	        $parameters['pNumProcesso']  = $this->processo->numprocesso();
	        $parameters['pURL']          = $MIOLO->GetConf('home.url');
	       // Removido por motivo de incompatibilidade de novo relatório
		   // $parameters['pTitulo']       = 'TRÂMITE DE PROCESSO';
	        $parameters['int_pUser']     = $MIOLO->login->idkey;
	        $parameters['int_andamento']     = 0;
	        $report->Execute('protocolo', 'ProcessoTramite', $parameters);
		}
		else
		{
            $this->addError('Não há andamentos');
		}
    }
} 
?>
