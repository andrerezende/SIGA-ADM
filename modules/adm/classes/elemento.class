<?
class BusinessAdmElemento extends Business
{
	var $idelemento;
	var $descricao;

	function BusinessAdmElemento($data=null)
	{
		$this->Business('sigaept',$data);
	}

	function GetById($id)
	{
		$this->idelemento = $id;
		$this->retrieve();
	}

	function ListAll()
	{
        $criteria =  $this->getCriteria();
        return $criteria->retrieveAsQuery();
	}

}
?>
