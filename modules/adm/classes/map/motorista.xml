<map>
    <moduleName>adm</moduleName>
    <className>motorista</className>
    <tableName>ad_motorista</tableName>
    <databaseName>sigaept</databaseName>
    <attribute>
        <attributeName>idMotorista</attributeName>
        <columnName>idmotorista</columnName>
		<idgenerator>seq_ad_motorista</idgenerator>
		<key>primary</key>
    </attribute>
    <attribute>
        <attributeName>carteiraHab</attributeName>
        <columnName>carteirahab</columnName>
    </attribute>
    <attribute>
        <attributeName>dataValidade</attributeName>
        <columnName>datavalidade</columnName>
		<converter>
			<converterName>DateConverter</converterName>
			<parameter>
				<parameterName>database</parameterName>
				<parameterValue>sigaept</parameterValue>
			</parameter>
		</converter>
    </attribute>
    <attribute>
        <attributeName>categoria</attributeName>
        <columnName>categoria</columnName>
    </attribute>
    <attribute>
        <attributeName>ativo</attributeName>
        <columnName>ativo</columnName>
    </attribute>
    <attribute>
        <attributeName>idPessoa</attributeName>
        <columnName>idpessoa</columnName>
    </attribute>
    <attribute>
        <attributeName>pessoa</attributeName>
    </attribute>

   <association>
		<toClassModule>common</toClassModule>
		<toClassName>pessoa</toClassName>
		<cardinality>oneToOne</cardinality>
		<target>pessoa</target>
		<retrieveAutomatic>true</retrieveAutomatic>
		<saveAutomatic>false</saveAutomatic>
		<entry>
			<fromAttribute>idPessoa</fromAttribute>
			<toAttribute>idpessoa</toAttribute>
		</entry>
   </association>
   <attribute>
		<attributeName>uos</attributeName>
	</attribute>
   <association>
      <toClassModule>adm</toClassModule>
      <toClassName>uo</toClassName>
      <associativeClassModule>adm</associativeClassModule>
      <associativeClassName>motoristauo</associativeClassName>
      <cardinality>manyToMany</cardinality>
      <target>uos</target>
	  <retrieveAutomatic>false</retrieveAutomatic>
	  <saveAutomatic>false</saveAutomatic>
	  <direction>
	     <fromAttribute>motoristas</fromAttribute>
	     <toAttribute>uos</toAttribute>
      </direction>
   </association>
</map>
