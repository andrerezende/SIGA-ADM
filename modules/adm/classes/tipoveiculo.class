<?
class BusinessAdmTipoVeiculo extends Business
{
	public $tipoVeiculo;
	public $descricao;
	public $valorKm;
    public $valorFixo;    

	function __construct($data = null)
	{
		parent::__construct('sigaept', $data);
	}

	function getById($id)
	{
		$this->tipoVeiculo = $id;
		$this->retrieve();
		return $this;
	}

	function listAll()
	{
        $criteria =  $this->getCriteria();
        return $criteria->retrieveAsQuery();
	}

}
?>
