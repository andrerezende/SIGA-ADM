    function listByUsuarioTipoReq($idUsuario,$idTipo)
    {
        $idTipoReq = (int)$idTipo;
        $sql = new sql ("distinct o.iduo,nome", 
        "ad_uo o
            inner join cm_usuario u on u.idusuario = o.idusuario
            inner join ad_requisita r on r.iduo = o.iduo
            inner join ad_tiporeq t on t.idtiporeq = r.idtiporeq");
        
        $sql->SetWhereAND('t.idtiporeq = '.$idTipoReq);
        $sql->SetWhereAND('o.idusuario = '.$idUsuario);

        $query = $this->Query($sql);
        return $query;
    }
    
