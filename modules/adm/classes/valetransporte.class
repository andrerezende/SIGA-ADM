<?
class BusinessAdmValeTransporte extends Business
{
    public $dataIni;
    public $dataFim;
    public $valor;

    function __construct($data = null)
    {
        parent::__construct('sigaept', $data);
    }

    function GetById($id)
    {
        $this->dataIni = $id;
        $this->retrieve();
        return $this;
    }

    function ListAll()
    {
        $criteria = $this->getCriteria();
        return $criteria->retrieveAsQuery();
    }

    function ListByNome($nome)
    {
        $criteria =  $this->getCriteria();
        $criteria->addCriteria('valor','>', "0");
		return $criteria->retrieveAsQuery();
    }
}
?>
