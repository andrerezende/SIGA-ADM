class BusinessAdmSubelemento extends Business
{
    var $idElemento;
    var $idSubElemento;
    var $descricao;

    function BusinessAdmSubelemento($data=null)
    {
        $this->Business('sigaept',$data);
    }

    function GetById($id,$sub)
    {
        $this->idElemento = $id;
        $this->idSubElemento = $sub;
        $this->retrieve();
    }

    function ListAll()
    {
        $criteria = $this->getCriteria();
        return $criteria->retrieveAsQuery();
    }
    function ListAll339030()
    {
        $criteria = $this->getCriteria();
	$criteria->AddColumnAttribute('idsubelemento');
	$criteria->AddColumnAttribute('descricao');
        $criteria->addCriteria('idelemento','=',339030);
	return $criteria->retrieveAsQuery();
    }
}
