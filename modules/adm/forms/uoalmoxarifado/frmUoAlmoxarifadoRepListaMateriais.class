<?
class frmUoAlmoxarifadoRepListaMateriais extends MForm
{
	protected $uoalmoxarifado;

    function __construct($objUo)
    {
        $this->uoalmoxarifado = $objUo;
	
		parent::__construct("Relatório de Lista de Materiais");
        $this->EventHandler();
    }

	function CreateFields()
	{
		$buttons = Array
		(
			new FormButton('btnGerarRelatorio', 'Gerar Relatório','pdf')
		);
		$this->SetButtons($buttons);
	}

	function btnGerarRelatorio_click()
	{
		global $MIOLO;
		$empenho = $MIOLO->getBusiness('adm', 'empenho',1);
        $report = $MIOLO->getUI()->GetReport('adm','repUoAlmoxarifadoListaDeMateriais',$this->uoalmoxarifado);
        $report->Generate();
	}




}
?>
