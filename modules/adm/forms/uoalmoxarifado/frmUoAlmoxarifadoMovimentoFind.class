<?php
class frmUoAlmoxarifadoMovimentoFind extends Form {

	/**
	 * Objeto UoAlmoxarifado
	 *
	 * @var BusinessAdmUo
	 */	
    protected $objUoAlmoxarifado;
    
    function __construct($objUoAlmoxarifado) {
        $this->objUoAlmoxarifado = $objUoAlmoxarifado;
        $this->Form('Movimentos');
        $this->EventHandler();
    }

    function CreateFields() {
        $grid = $this->manager->GetUI()->GetGrid('adm','gridUoAlmoxarifadoMovimento',$this->objUoAlmoxarifado);

        $busca->showLabel = true;
        $fields = array(
            $busca,
            new Spacer('5px'),
            $grid
        );
        $this->SetFields($fields);
        $this->defaultButton = false;
    }
 }