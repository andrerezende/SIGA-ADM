<?
class frmUoAcessoAlmoxarifadoFind extends Form
{
    protected $objUo;
    
    function __construct($objUo)
    {
        $this->objUo = $objUo;
        $this->Form('Acesso a Almoxarifado');
        $this->EventHandler();
    }

    function CreateFields()
    {
        $grid = $this->manager->GetUI()->GetGrid('adm','gridUoAcessoAlmoxarifado',$this->objUo);
        $this->AddField($grid);
        $this->defaultButton = false;
    }
 }
?>
