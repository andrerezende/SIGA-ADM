<?
class frmUoOrcamentoInternoTipoRequisicoesRealizadasFind extends Form
{
    protected $objUo;
	
    function __construct($objUo)
    {
       $this->objUo = $objUo;
       $this->Form('Pesquisar requisições realizadas');
       $this->EventHandler();
    }

	function CreateFields()
    {
        $grid = $this->manager->GetUI()->GetGrid('adm','gridUoOrcamentoInternoTipoRequisicoesRealizadas',$this->objUo);
        $this->AddField($grid);
        $this->defaultButton = false;
	}
} 
?>

