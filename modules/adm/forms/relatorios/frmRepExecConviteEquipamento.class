<?
class frmRepExecConviteEquipamento extends MForm
{
    

    function __construct()
    {   
		parent::__construct('Convite Equipamentos');
        $this->EventHandler();
	}

	function createFields()
	{
        
		$buttons = Array
		(
			new FormButton('btnRep', 'Gerar Convite Equipamento','pdf'),
		);
		$this->SetButtons($buttons);
	}

    function btnRep_click()
    {   global $MIOLO;

        $report = $MIOLO->getUI()->GetReport('adm','repConviteEquipamento');
        $report->Generate();
    }
}
?>