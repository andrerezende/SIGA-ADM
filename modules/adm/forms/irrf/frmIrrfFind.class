<?php
class frmIrrfFind extends MForm
{
    function __construct()
    {
		global $MIOLO, $module;
        parent::__construct('Manutenção da Alíquota de IRRF');
		$this->SetClose($MIOLO->GetActionURL($module,'main:manutencao'));
        $this->EventHandler();
    }
	
    
    function createFields()
    {
        global $MIOLO, $action;

		$ui = $MIOLO->getUI();
        $grid = $ui->getGrid('adm','gridIrrf');
        $grid->setTitle(NULL);
        
        $lnkNew = new  MLink('lkNew', '&nbsp;', '','Inserir Novo IRRF');
		$lnkNew->setAction('adm', 'main:irrf:new');
        
        $fields = array($lnkNew, $grid);
      
        $this->setFields($fields);
        $this->defaultButton = false;
    }
 }
?>
