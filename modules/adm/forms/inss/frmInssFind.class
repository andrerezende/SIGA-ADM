<?
class frmInssFind extends MForm
{
	function __construct()
	{
		global $MIOLO, $module;
	parent:: __construct('Manutenção de alíquota do INSS');
	$this->SetClose($MIOLO->GetActionURL($module,'main:manutencao'));
	$this->EventHandler();
	}
	function CreateFields()
	{
		global $MIOLO, $action;
	
		$ui = $MIOLO->GetUI();
		$grid = $ui->GetGrid('adm','gridInss');
		$grid->SetTitle(NULL);
		$lnkNew = new MLink('lnkNew','&nbsp;','','Inserir nova alíquota do INSS');
		$lnkNew->SetAction('adm','inss:new');
		$fields = array(
				array($lnkNew),$grid);
		$this->SetFields($fields);
		$this->defaultButton= false;
	}
}
?>
