<?
class frmEmpenhoEntregaEmpenhados extends MForm
{
    protected $empenho;
    function __construct($objEmpenho)
    {
        global $MIOLO;
        $this->empenho = $objEmpenho;
        parent::__construct('Materiais a entregar');
        $this->SetClose($this->manager->GetActionURL('adm','main:empenho'));
        $this->eventHandler();
    }
    function CreateFields()
    {
        $objEmpenho = $this->empenho;
        $this->addField($this->manager->getUI()->getGrid('adm','gridEmpenhoEntregaEmpenhados',$objEmpenho));
        $this->defaultButton = false;
    }
}
?>
