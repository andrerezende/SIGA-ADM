	<queryString><![CDATA[SELECT 
	r.idrequisicao,
	t.descricao,
	l.numero,
	u.sigla, 
	(ir.quantpedida * ir.precoestimado) as valor,
	e.idempenho as empenho
FROM 
	ad_requisicao r,
	ad_itemreq ir,
	ad_licitacao l,
	ad_empenhado ea,
	ad_empenho e,
	ad_tiporeq t,
	ad_uo u
WHERE 
	r.suplementar = 'S' and 
	r.idrequisicao = ir.idrequisicao and 
	ir.idlicitacao = l.idlicitacao and 
	r.tiporequisicao = t.idtiporeq and 
	r.iduoreq = u.iduo and
	ea.idlicitacao = l.idlicitacao and
	e.idempenho = ea.idempenho

UNION
 
select 
	r.idrequisicao, 
	t.descricao, 
	l.numero, 
	u.sigla,
	(iserv.quantidade * iserv.valorest) as valor,
	e.idempenho as empenho
FROM 
	ad_requisicao r, 
	ad_itemreqservext iserv, 
	ad_licitacao l, 
	ad_empenho e,
	ad_tiporeq t, 
	ad_empenhado ea,
	ad_uo u 
WHERE 
	r.suplementar = 'S' and 
	r.idrequisicao = iserv.idrequisicao and 
	iserv.idlicitacao = l.idlicitacao and 
	r.tiporequisicao = t.idtiporeq and 
	r.iduoreq = u.iduo and
	ea.idlicitacao = l.idlicitacao and
	e.idempenho = ea.idempenho


UNION 

select 
	r.idrequisicao, 
	t.descricao, 
	l.numero, 
	u.sigla,
	(ib.quantpedida * ib.precoestimado) as valor,
	e.idempenho as empenho
FROM 
	ad_requisicao r, 
	ad_itemreqbib ib, 
	ad_licitacao l, 
	ad_empenhado ea,
	ad_empenho e,
	ad_tiporeq t, 
	ad_uo u 
WHERE 
	r.suplementar = 'S' and 
	r.idrequisicao = ib.idrequisicao and 
	ib.idlicitacao = l.idlicitacao and 
	r.tiporequisicao = t.idtiporeq and 
	r.iduoreq = u.iduo and
	ea.idlicitacao = l.idlicitacao and
	e.idempenho = ea.idempenho
;]]></queryString>

