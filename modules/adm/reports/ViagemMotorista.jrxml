	<queryString><![CDATA[select
	p.nome as Motorista,
	count(i.idrequisicao) as Quantidade,
	(mun.municipio ||'/'|| mun.iduf) as Municipio
from 
	ad_motorista m,
	cm_pessoa p,
	ad_itemreqveiculo i,
	ad_requisicao r,
	cm_municipio mun
where
	m.idpessoa = p.idpessoa and
	m.idmotorista = i.idmotorista and
	i.idrequisicao = r.idrequisicao and
	i.idmunicipiodestino = '5819' and
	mun.idmunicipio=i.idmunicipiodestino and
	r.status='G'
group by Motorista, Municipio, mun.iduf;]]></queryString>

