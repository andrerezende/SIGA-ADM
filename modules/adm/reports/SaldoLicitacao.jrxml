	<queryString><![CDATA[SELECT
	l.numero as Numero,
	il.item as Num, 
	m.descricao as Descricao,
	ir.quantpedida as Quantidade, 
	m.unidade as Unidade, 
	mp.preco as Preco,
	i.nome as Empresa,
	Quantidade * Preco  as Total,
	CAST(m.idelemento AS text)||CAST(m.idsubelemento AS text) as Rubrica

FROM
	ad_licitacao l,
	ad_itemlicitacao il,
	ad_itemreq ir,
	ad_material m,
	ad_mapalicitacao mp,
	ad_fornecedor f,
	cm_instituicao i

WHERE
	il.idlicitacao = l.idlicitacao and 
	il.idlicitacao = ir.idlicitacao and
	ir.idmaterial = m.idmaterial and
	l.idlicitacao = mp.idlicitacao and
	mp.idfornecedor = f.idfornecedor and
	f.idinstituicao = i.idinstituicao and
	mp.vencedor = '1' and
	l.numero = $P{numlicitacao};]]></queryString>

