<?
/**
 * Link Liquidacao class
 */
global $MIOLO;
$MIOLO->Uses('controls/linkliquidacao.class','adm');

class LinkPagamentoLiquidacao extends LinkLiquidacao
{
    public function __construct($name='', $liquidacao)
    {
        parent::__construct($name,$liquidacao);
    }

    public function setAction()
    {
        $this->hyperlink->SetAction('adm','main:pagamento',$this->object->idLiquidacao);
        $this->imagelink->SetAction('adm','main:pagamento',$this->object->idLiquidacao);
    }
}
?>
