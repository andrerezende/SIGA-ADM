<?php
class BusinessCommonUsuarioGrupofr  extends Business
{
    public $grp_codigo;
    public $sis_codigo;
    public $usr_codigo;

    function __construct($data = null)
    {
        parent::__construct('sigaept', $data);
    }

    function OnCreate($data)
    {
        if (empty($data))
        {
//            $this->initialize();
        }
        else
        {
            $this->GetById($data);
        }
    }

    function GetById($idusr, $idgrp, $idsis)
    {
      $this->usr_codigo = $idusr;
      $this->grp_codigo = $idgrp;
      $this->sis_codigo = $idsis;
      $this->retrieve();
      return $this;
    }
   

    
}