<?
class frmCasosDeUsoFind extends MForm
{
	function __construct()
	{
		parent::__construct('Casos de Uso');
		$this->setClose($this->manager->getActionURL('common','main'));
		$this->eventHandler();
	}
	function createFields()
	{
        $doc = $this->manager->getBusiness('common','documentation');
        $qryModules = $doc->listModules();
		$ui = $this->manager->GetUI();
		$grid = $ui->GetGrid('common','gridCasosDeUso');
		$grid->setTitle(NULL);
		$findButton = new MButtonFind('');
		$findButton->setName('btnFind');
		$fields = array(
            array(
               new MSelection('modulo','','M&oacute;dulo',$qryModules->result),
               $findButton,
            ),
            $grid
        );
		$this->SetFields($fields);
		$this->defaultButton = false;
	}
}
?>