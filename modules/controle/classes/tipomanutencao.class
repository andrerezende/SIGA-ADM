<?
class BusinessControleTipoManutencao extends Business
{
    public $idTipoManutencao;
    public $tipoManutencao;
        
    function __construct($data=NULL)
    {
        $this->Business('sigaept',$data);
    }
    function GetById($id)
    {
		$this->idTipoManutencao = $id;
		$this->retrieve();
    }
    function setData($data)
    {
        $this->tipoManutencao = strtoUpper($data->tipoManutencao);
        
    }
    function getData()
    {
		$data->idTipoManutencao = $this->idTipoManutencao;
		$data->tipoManutencao   = $this->tipoManutencao;
		return $data;
    }
    function listAll()
    {
        global $MIOLO;
        
        $criteria =  $this->getCriteria();
        $criteria->addColumnAttribute('idTipoManutencao');
        $criteria->addColumnAttribute('tipoManutencao');
        $criteria->addOrderAttribute('idTipoManutencao');
        return $criteria->retrieveAsQuery();
    }	
}
?>